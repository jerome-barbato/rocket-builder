{
  "name": "metabolism/rocket-builder",
  "version": "3.5.1",
  "description": "Gulp tasks to compile scss, js, and templates. Metabolism framework included.",
  "type": "composer-plugin",
  "license": "AGPL-3.0",
  "authors": [
    {
      "name": "Jérôme BARBATO",
      "email": "jerome@metabolism.fr"
    },
    {
      "name": "Paul COUDEVILLE",
      "email": "paul@metabolism.fr"
    }
  ],
  "require": {
    "composer-plugin-api": "^1.0",
    "metabolism/rocket-installer": "1.0.x"
  },
  "extra": {
    "class": "Rocket\\Composer\\BuilderPlugin",
    "file-management": {
      "symlink": {
        "metabolism/rocket-builder": {
          "doc": "doc/builder"
        }
      }
    },
    "branch-alias": {
      "dev-3.5.x": "3.5.x-dev",
      "dev-3.4.x": "3.4.x-dev",
      "dev-3.3.x": "3.3.x-dev",
      "dev-3.2.x": "3.2.x-dev"
    }
  },
  "autoload": {
    "psr-4": {
      "Rocket\\": "src/"
    }
  }
}
